{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "organizationUrl": {
      "value": "${GITHUB_ORG_URL}"
    },
    "repositories": {
      "value": ["${GITHUB_REPOSITORY_NAME}"]
    },
    "maximumConcurrency": {
      "value": "${MAXIMUM_CONCURRENCY=1}"
    },
    "vmSize": {
      "value": "${VM_SIZE=Standard_D2s_v3}"
    },
    "imageName": {
      "value": "${IMAGE_NAME=ubuntu-latest}"
    },
    "devOpsInfrastructureObjectId": {
      "value": "${DEVOPSINFRASTRUCTURE_PRINCIPLE_ID}"
    }
  }
}
